<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.android.navigation.TitleFragment"
        android:label="StartTheApp"
        tools:layout="@layout/fragment_title">
        <action
            android:id="@+id/action_titleFragment_to_fetchFragment"
            app:destination="@id/fetchFragment" />
    </fragment>
    <fragment
        android:id="@+id/fetchFragment"
        android:name="com.windsake.codefetcherapp.ui.FetchFragment"
        android:label="FetchFragment" />
</navigation>